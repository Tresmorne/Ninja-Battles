	proc
		Levelup()
			if(src.exp>=src.max_exp)
				src.level+=1
				src.exp -= 150
				src.max_exp+=rand(12,17)
				src.Mchakra+=rand(35,60)
				src.taiexp+=rand(15,20)
				src.ninexp+=rand(15,20)
				src.genexp+=rand(15,20)
				src<<"<font size = 2><b><font color = blue>You leveled up!!"
				src.maxhealth+=rand(40,70)
				src.taiup()
				src.ninup()
				src.genup()
				src.nin=src.Mnin
				src.tai=src.Mtai
				src.gen=src.Mgen
		taiup()
			if(src.taiexp>=src.mtaiexp)
				src.mtaiexp += rand(1,3)
				src.tailvl+=1
				src.taiexp = 0
				src.level+=1
				src.Mtai+=rand(20,40)
				src.tai=src.Mtai
				src<<"<font size = 2><b><font color = blue>Your Tai leveled up!!"
		ninup()
			if(src.ninexp>=src.mninexp)
				src.mninexp += rand(1,3)
				src.ninlvl+=1
				src.ninexp = 0
				src.Mnin+=rand(20,40)
				src.level+=1
				src.nin=src.Mnin
				src<<"<font size = 2><b><font color = blue>Your Nin leveled up!!"
		genup()
			if(src.genexp>=src.mgenexp)
				src.mgenexp += rand(1,3)
				src.genlvl+=1
				src.level+=1
				src.genexp = 0
				src.Mgen+=rand(20,40)
				src.gen=src.Mgen
				src<<"<font size = 2><b><font color = blue>Your Gen leveled up!!"

