	proc
		Death(mob/M)
			if(istype(src,/mob/pet)&&src.isdog&&!src.owned)
				del(src)
			else if(src.health<=0&&src.GOTCS&&!src.incs)
				src.random=rand(1,6)
				if(src.random==4)
					for(var/obj/shibari/K in world)
						if(K.owner==M)
							del(K)
					for(var/obj/DemonMirror/S in world)
						if(S.owner==M)
							src.hyoushou=0
							M.hyoushou=0
							del(S)
					for(var/obj/DenseThing/U in world)
						if(U.owner==M)
							src.hyoushou=0
							M.hyoushou=0
							del(U)
					for(var/obj/shibaritrail/T in world)
						if(T.owner==M)
							del(T)
					view(src)<<"[src] enrages and unlocks their cursed seal!"
					src.Cursed_Seal()
					return
				else if(src.health <= 0)
					for(var/obj/shibari/K in world)
						if(K.owner==src)
							del(K)
						if(K.owner==M)
							del(K)
					for(var/obj/DemonMirror/S in world)
						if(S.owner==src)
